<div class="container">
    <div class="row landing-page__zones">
        <div class="col-md-4">
            <div class="landing-page__zone landing-page__zone--{{ zones[0].id }} landing-page__zone--left" data-ez-zone-id="{{ zones[0].id }}">
                {% if zones[0].blocks %}
                    {% include '@ezdesign/zone/zone.html.twig' with {'blocks': zones[0].blocks} %}
                {% endif %}
            </div>
        </div>
        <div class="col-md-8">
            <div class="landing-page__zone landing-page__zone--{{ zones[1].id }} landing-page__zone--right" data-ez-zone-id="{{ zones[1].id }}">
                {% if zones[1].blocks %}
                    {% include '@ezdesign/zone/zone.html.twig' with {'blocks': zones[1].blocks} %}
                {% endif %}
            </div>
        </div>
    </div>
</div>
