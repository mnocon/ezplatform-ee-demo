<div class="container">
    <div class="landing-page__zones">
        <div class="landing-page__zone landing-page__zone--{{ zones[0].id }}" data-ez-zone-id="{{ zones[0].id }}">
            {% if zones[0].blocks %}
                {% include '@ezdesign/zone/zone.html.twig' with {'blocks': zones[0].blocks} %}
            {% endif %}
        </div>
    </div>
    <div class="row landing-page__zones">
        <div class="col-4">
            <div class="landing-page__zone landing-page__zone--{{ zones[1].id }} landing-page__zone--left" data-ez-zone-id="{{ zones[1].id }}">
                {% if zones[1].blocks %}
                    {% include '@ezdesign/zone/zone.html.twig' with {'blocks': zones[1].blocks} %}
                {% endif %}
            </div>
        </div>
        <div class="col-8">
            <div class="landing-page__zone landing-page__zone--{{ zones[2].id }} landing-page__zone--right" data-ez-zone-id="{{ zones[2].id }}">
                {% if zones[2].blocks %}
                    {% include '@ezdesign/zone/zone.html.twig' with {'blocks': zones[2].blocks} %}
                {% endif %}
            </div>
        </div>
    </div>
</div>
